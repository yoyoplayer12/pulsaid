{
    "routes": [
      {
        "route": "/.well-known/**",
        "statusCode": 404,
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/**",
        "serve": "404.html",
        "statusCode": 404
      },
      {
        "route": "/api/**",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/",
        "serve": "index.html",
        "statusCode": 200
      }
    ]
  }
  