{
  "routes": [
    {
      "route": "/.well-known/**",
      "statusCode": 404,
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/**",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/en/*",
      "serve": "/en/404.html",
      "statusCode": 404
    },
    {
      "route": "/nl/*",
      "serve": "/nl/404.html",
      "statusCode": 404
    },
    {
      "route": "/en/**",
      "serve": "/en/index.html",
      "statusCode": 200
    },
    {
      "route": "/nl/**",
      "serve": "/nl/index.html",
      "statusCode": 200
    },
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/**",
      "serve": "/en/404.html",
      "statusCode": 404
    }
  ]
}